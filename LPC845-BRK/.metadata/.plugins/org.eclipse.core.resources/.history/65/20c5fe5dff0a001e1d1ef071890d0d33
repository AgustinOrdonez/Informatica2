#ifndef TYPES_H_
#define TYPES_H_

#define __I volatile const
#define __O volatile
#define __IO volatile

typedef		unsigned char 		uint8_t;
typedef		char 				int8_t;
typedef		short unsigned int 	uint16_t;
typedef		short  int 			int16_t;
typedef		unsigned int 		uint32_t;
typedef		int 				int32_t;
#endif /* TYPES_H_ */
